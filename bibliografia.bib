editor = {Thornton, Janet},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Xie, Bourne - 2005 - Functional coverage of the human genome by existing structures, structural genomics targets, and homology models.pdf:pdf},
issn = {1553-734X},
journal = {PLoS computational biology},
keywords = {Databases, Protein,Disease,Genome, Human,Genome, Human: genetics,Genomics,Humans,Models, Molecular,Structural Homology, Protein},
month = {aug},
