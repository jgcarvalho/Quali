keywords = {Bacteriophage M13,Bacteriophage M13: genetics,Computational Biology,DNA Primers,DNA Primers: genetics,Humans,Microarray Analysis,PDZ Domains,PDZ Domains: genetics,PDZ Domains: physiology,Peptide Library,Protein Interaction Mapping,Protein Interaction Mapping: methods,Proteomics,Proteomics: methods},
