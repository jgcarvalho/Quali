doi = {10.1002/prot.20008},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Karchin, Cline, Karplus - 2004 - Evaluation of local structure alphabets based on residue burial.pdf:pdf},
