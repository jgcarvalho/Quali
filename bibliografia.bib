primaryClass = {gr-qc},
title = {{Why most published research findings are false}},
volume = {2},
year = {2005}
}
@article{Shen2013b,
abstract = {A new program, TALOS-N, is introduced for predicting protein backbone torsion angles from NMR chemical shifts. The program relies far more extensively on the use of trained artificial neural networks than its predecessor, TALOS+. Validation on an independent set of proteins indicates that backbone torsion angles can be predicted for a larger, ≥90 {\%} fraction of the residues, with an error rate smaller than ca 3.5 {\%}, using an acceptance criterion that is nearly two-fold tighter than that used previously, and a root mean square difference between predicted and crystallographically observed (ϕ, $\psi$) torsion angles of ca 12{\textordmasculine}. TALOS-N also reports sidechain $\chi$(1) rotameric states for about 50 {\%} of the residues, and a consistency with reference structures of 89 {\%}. The program includes a neural network trained to identify secondary structure from residue sequence and chemical shifts.},
