pages = {132--45},
pmid = {1978340},
title = {{Tertiary structural constraints on protein evolutionary diversity: templates, key residues and structure prediction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/1978340},
volume = {241},
year = {1990}
}
@article{Wu2008a,
abstract = {We develop a new threading algorithm MUSTER by extending the previous sequence profile-profile alignment method, PPA. It combines various sequence and structure information into single-body terms which can be conveniently used in dynamic programming search: (1) sequence profiles; (2) secondary structures; (3) structure fragment profiles; (4) solvent accessibility; (5) dihedral torsion angles; (6) hydrophobic scoring matrix. The balance of the weighting parameters is optimized by a grading search based on the average TM-score of 111 training proteins which shows a better performance than using the conventional optimization methods based on the PROSUP database. The algorithm is tested on 500 nonhomologous proteins independent of the training sets. After removing the homologous templates with a sequence identity to the target {\textgreater}30{\%}, in 224 cases, the first template alignment has the correct topology with a TM-score {\textgreater}0.5. Even with a more stringent cutoff by removing the templates with a sequence identity {\textgreater}20{\%} or detectable by PSI-BLAST with an E-value {\textless}0.05, MUSTER is able to identify correct folds in 137 cases with the first model of TM-score {\textgreater}0.5. Dependent on the homology cutoffs, the average TM-score of the first threading alignments by MUSTER is 5.1-6.3{\%} higher than that by PPA. This improvement is statistically significant by the Wilcoxon signed rank test with a P-value {\textless} 1.0 x 10(-13), which demonstrates the effect of additional structural information on the protein fold recognition. The MUSTER server is freely available to the academic community at http://zhang.bioinformatics.ku.edu/MUSTER.},
author = {Wu, Sitao and Zhang, Yang},
doi = {10.1002/prot.21945},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wu, Zhang - 2008 - MUSTER Improving protein sequence profile-profile alignments by using multiple sources of structure information.pdf:pdf},
issn = {1097-0134},
journal = {Proteins},
keywords = {Algorithms,Databases, Protein,Models, Molecular,Protein Conformation,Proteins,Proteins: chemistry,Sequence Alignment},
month = {aug},
number = {2},
pages = {547--56},
pmid = {18247410},
title = {{MUSTER: Improving protein sequence profile-profile alignments by using multiple sources of structure information.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2666101{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {72},
year = {2008}
}
@article{Shi2001,
abstract = {FUGUE, a program for recognizing distant homologues by sequence-structure comparison (http://www-cryst.bioc.cam.ac.uk/fugue/), has three key features. (1) Improved environment-specific substitution tables. Substitutions of an amino acid in a protein structure are constrained by its local structural environment, which can be defined in terms of secondary structure, solvent accessibility, and hydrogen bonding status. The environment-specific substitution tables have been derived from structural alignments in the HOMSTRAD database (http://www-cryst.bioc. cam.ac.uk/homstrad/). (2) Automatic selection of alignment algorithm with detailed structure-dependent gap penalties. FUGUE uses the global-local algorithm to align a sequence-structure pair when they greatly differ in length and uses the global algorithm in other cases. The gap penalty at each position of the structure is determined according to its solvent accessibility, its position relative to the secondary structure elements (SSEs) and the conservation of the SSEs. (3) Combined information from both multiple sequences and multiple structures. FUGUE is designed to align multiple sequences against multiple structures to enrich the conservation/variation information. We demonstrate that the combination of these three key features implemented in FUGUE improves both homology recognition performance and alignment accuracy.},
author = {Shi, J and Blundell, T L and Mizuguchi, K},
doi = {10.1006/jmbi.2001.4762},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Shi, Blundell, Mizuguchi - 2001 - FUGUE sequence-structure homology recognition using environment-specific substitution tables and struc.pdf:pdf},
issn = {0022-2836},
journal = {Journal of molecular biology},
keywords = {Algorithms,Amino Acid Substitution,Computational Biology,Computational Biology: methods,Conserved Sequence,Databases as Topic,Hydrogen Bonding,Internet,Protein Folding,Protein Structure, Secondary,Proteins,Proteins: chemistry,Sensitivity and Specificity,Sequence Alignment,Sequence Homology, Amino Acid,Software,Solvents},
month = {jun},
