@article{Zarrine-Afsar2005,
abstract = {Theoretical and experimental studies of protein folding have suggested that the topology of the native state may be the most important factor determining the folding pathway of a protein, independent of its specific amino acid sequence. To test this concept, many experimental studies have been carried out with the aim of comparing the folding pathways of proteins that possess similar tertiary structures, but divergent sequences. Many of these studies focus on quantitative comparisons of folding transition state structures, as determined by Phi(f) value analysis of folding kinetic data. In some of these studies, folding transition state structures are found to be highly conserved, whereas in others they are not. We conclude that folds displaying more conserved transition state structures may have the most restricted number of possible folding pathways and that folds displaying low transition state structural conservation possess many potential pathways for reaching the native state.},
author = {Zarrine-Afsar, Arash and Larson, Stefan M and Davidson, Alan R},
doi = {10.1016/j.sbi.2005.01.011},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Zarrine-Afsar, Larson, Davidson - 2005 - The family feud do proteins with similar structures fold via the same pathway.pdf:pdf},
issn = {0959-440X},
journal = {Current opinion in structural biology},
keywords = {Binding Sites,Chemical,Conserved Sequence,Kinetics,Models,Molecular,Motion,Protein,Protein Binding,Protein Conformation,Protein Folding,Protein: methods,Proteins,Proteins: chemistry,Proteins: classification,Sequence Alignment,Sequence Alignment: methods,Sequence Analysis,Structure-Activity Relationship},
month = {feb},
