@Article{Dill:2003,
author="Dill, A. Ken
and Ozkan, Banu S.
and Shell, Scott M.
and Weikl, R. Thomas",
title="The protein folding problem",
journal="Annual review of biophysics",
year="2002",
volume="37",
pages="289--316",
keywords="computer simulation; models, chemical; models, molecular; protein conformation; protein folding; proteins; proteins: chemistry; proteins: ultrastructure",
abstract="The ``protein folding problem'' consists of three closely related puzzles: (a) What is the folding code? (b) What is the folding mechanism? (c) Can we predict the native structure of a protein from its amino acid sequence? Once regarded as a grand challenge, protein folding has seen great progress in recent years. Now, foldable proteins and nonbiological polymers are being designed routinely and moving toward successful applications. The structures of small proteins are now often well predicted by computer methods. And, there is now a testable explanation for how a protein can fold so quickly: A protein solves its large global optimization problem as a series of smaller local optimization problems, growing and assembling the native structure from peptide fragments, local structures first",
issn="1936122X",
doi="10.1146/annurev.biophys.37.092707.153558",
url="http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2443096&tool=pmcentrez&rendertype=abstract"
}

@Article{Doyle:2006,
author="Doyle, S. Patricia
and Zhou, M. Yuan
and Hsieh, Ivy
and Greenbaum, C. Doron
and McKerrow, H. James
and Engel, C. Juan",
title="The Trypanosoma cruzi protease cruzain mediates immune evasion",
journal="PLoS pathogens",
year="2005",
publisher="Public Library of Science",
pages="1002139--1002139",
keywords="animals; arginase; arginase: biosynthesis; cysteine endopeptidases; cysteine endopeptidases: deficiency; cysteine endopeptidases: physiology; dipeptides; dipeptides: pharmacology; humans; i-kappa b proteins; i-kappa b proteins: metabolism; immune evasion; immune evasion: physiology; interleukin-12; interleukin-12: biosynthesis; lipopolysaccharides; lipopolysaccharides: pharmacology; macrophage activation; macrophage activation: drug effects; macrophages; macrophages: drug effects; macrophages: metabolism; macrophages: parasitology; mice; nf-kappa b; nf-kappa b: metabolism; protozoan proteins; protozoan proteins: physiology; trypanosoma cruzi; trypanosoma cruzi: drug effects; trypanosoma cruzi: genetics; vinyl compounds; vinyl compounds: pharmacology",
abstract="Trypanosoma cruzi is the causative agent of Chagas' disease. Novel chemotherapy with the drug K11777 targets the major cysteine protease cruzain and disrupts amastigote intracellular development. Nevertheless, the biological role of the protease in infection and pathogenesis remains unclear as cruzain gene knockout failed due to genetic redundancy. A role for the T. cruzi cysteine protease cruzain in immune evasion was elucidated in a comparative study of parental wild type- and cruzain-deficient parasites. Wild type T. cruzi did not activate host macrophages during early infection (\&lt;60 min) and no increase in ?P i?B was detected. The signaling factor NF-?B P65 colocalized with cruzain on the cell surface of intracellular wild type parasites, and was proteolytically cleaved. No significant IL-12 expression occurred in macrophages infected with wild type T. cruzi and treated with LPS and BFA, confirming impairment of macrophage activation pathways. In contrast, cruzain-deficient parasites induced macrophage activation, detectable i?B phosphorylation, and nuclear NF-?B P65 localization. These parasites were unable to develop intracellularly and survive within macrophages. IL 12 expression levels in macrophages infected with cruzain-deficient T. cruzi were comparable to LPS activated controls. Thus cruzain hinders macrophage activation during the early (\&lt;60 min) stages of infection, by interruption of the NF-?B P65 mediated signaling pathway. These early events allow T. cruzi survival and replication, and may lead to the spread of infection in acute Chagas' disease",
issn="15537374",
doi="10.1371/journal.ppat.1002139",
url="http://dx.plos.org/10.1371/journal.ppat.1002139"
}

@Article{McGuffin:1995,
author="McGuffin, J. L.
and Bryson, K.
