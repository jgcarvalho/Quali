journal = {Nucleic acids research},
keywords = {Amino Acid Motifs,Amino Acid Sequence,Bacteriophages,Bacteriophages: genetics,Cell Surface Display Techniques,Consensus Sequence,Ligands,Peptides, Cyclic,Peptides, Cyclic: chemistry,Peptides, Cyclic: isolation {\&} purification,Peptides, Cyclic: metabolism,Sequence Analysis, Protein,Sequence Analysis, Protein: methods,Software},
month = {dec},
number = {22},
pages = {e169},
pmid = {25348396},
title = {{Identification of target-binding peptide motifs by high-throughput sequencing of phage-selected peptides.}},
