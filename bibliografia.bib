@article{Podtelezhnikov2009a,
abstract = {Efficient and accurate reconstruction of secondary structure elements in the context of protein structure prediction is the major focus of this work. We present a novel approach capable of reconstructing alpha-helices and beta-sheets in atomic detail. The method is based on Metropolis Monte Carlo simulations in a force field of empirical potentials that are designed to stabilize secondary structure elements in room-temperature simulations. Particular attention is paid to lateral side-chain interactions in beta-sheets and between the turns of alpha-helices, as well as backbone hydrogen bonding. The force constants are optimized using contrastive divergence, a novel machine learning technique, from a data set of known structures. Using this approach, we demonstrate the applicability of the framework to the problem of reconstructing the overall protein fold for a number of commonly studied small proteins, based on only predicted secondary structure and contact map. For protein G and chymotrypsin inhibitor 2, we are able to reconstruct the secondary structure elements in atomic detail and the overall protein folds with a root mean-square deviation of {\textless}10 A. For cold-shock protein and the SH3 domain, we accurately reproduce the secondary structure elements and the topology of the 5-stranded beta-sheets, but not the barrel structure. The importance of high-quality secondary structure and contact map prediction is discussed.},
author = {Podtelezhnikov, Alexei A and Wild, David L},
doi = {10.1016/j.bpj.2009.02.057},
file = {:home/jgcarvalho/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Podtelezhnikov, Wild - 2009 - Reconstruction and stability of secondary structure elements in the context of protein structure predictio.pdf:pdf},
issn = {1542-0086},
journal = {Biophysical journal},
keywords = {Algorithms,Artificial Intelligence,Bacterial Proteins,Bacterial Proteins: chemistry,Computer Simulation,Escherichia coli,Hydrogen Bonding,Models, Chemical,Models, Molecular,Monte Carlo Method,Nerve Tissue Proteins,Nerve Tissue Proteins: chemistry,Peptides,Peptides: chemistry,Plant Proteins,Plant Proteins: chemistry,Protein Stability,Protein Structure, Secondary,Temperature,src Homology Domains,src-Family Kinases,src-Family Kinases: chemistry},
month = {jun},
