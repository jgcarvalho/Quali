journal = {PLoS pathogens},
keywords = {Animals,Arginase,Arginase: biosynthesis,Cysteine Endopeptidases,Cysteine Endopeptidases: deficiency,Cysteine Endopeptidases: physiology,Dipeptides,Dipeptides: pharmacology,Humans,I-kappa B Proteins,I-kappa B Proteins: metabolism,Immune Evasion,Immune Evasion: physiology,Interleukin-12,Interleukin-12: biosynthesis,Lipopolysaccharides,Lipopolysaccharides: pharmacology,Macrophage Activation,Macrophage Activation: drug effects,Macrophages,Macrophages: drug effects,Macrophages: metabolism,Macrophages: parasitology,Mice,NF-kappa B,NF-kappa B: metabolism,Protozoan Proteins,Protozoan Proteins: physiology,Trypanosoma cruzi,Trypanosoma cruzi: drug effects,Trypanosoma cruzi: genetics,Vinyl Compounds,Vinyl Compounds: pharmacology},
