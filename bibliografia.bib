journal = {Biophysical journal},
keywords = {Computational Biology,Computational Biology: methods,Models, Statistical,Proteins,Proteins: chemistry,Thermodynamics},
month = {oct},
number = {8},
pages = {2043--52},
pmid = {22004759},
title = {{GOAP: a generalized orientation-dependent, all-atom statistical potential for protein structure prediction.}},
