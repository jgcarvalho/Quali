keywords = {Amino Acid Sequence,Amino Acids,Amino Acids: analysis,Biophysical Phenomena,Biophysics,Databases, Factual,Molecular Structure,Protein Folding,Protein Structure, Secondary,Proteins,Proteins: chemistry},
month = {sep},
